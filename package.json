{
  "name": "now-dart",
  "description": "Community based builder for using dart on the now/zeit platform",
  "version": "1.0.1",
  "license": "MIT",
  "main": "./dist/index",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "tsc",
    "test": "tsc && jest",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "dist",
    "launcher.rs"
  ],
  "dependencies": {
    "@now/build-utils": "^0.9.6",
    "execa": "^1.0.0",
    "fs-extra": "^7.0.1",
    "now": "^16.4.4"
  },
  "devDependencies": {
    "@types/execa": "^0.9.0",
    "@types/fs-extra": "^7.0.0",
    "husky": "^3.0.2",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.1",
    "ms": "^2.1.2",
    "node-fetch": "^2.6.0",
    "prettier": "^1.18.2",
    "typescript": "3.5.2"
  },
  "husky": {
    "hooks": {
      "precommit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,md,json}": [
      "prettier --write",
      "git add"
    ]
  }
}
